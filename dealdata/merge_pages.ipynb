   "metadata": {},
   "outputs": [],
   "source": [
    "ipt = \"./data_50/\"\n",
    "data = []\n",
    "for file in os.listdir(ipt):\n",
    "    frame_page = pd.read_csv(ipt + file)\n",
    "    data.append(frame_page)\n",
    "result = pd.concat(data)\n",
    "result.to_csv(\"./raw_data/raw_data.csv\")"
   ]
